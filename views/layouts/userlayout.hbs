  <body>
    <header>
      <nav class="navbar navbar-expand-lg "style="height: 6.5675rem; background-color: #6A9739;">
        <div class="container">
          <a class="navbar-brand" href="#">
            <img src="/images/organic-store-white-logo.png" alt="Organic"width="80" height="40">
          </a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse"data-bs-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup" aria-expanded="false"aria-label="Toggle navigation">
              <span class="navbar-toggler-icon">
          </button>
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav d-flex flex-row align-items-center">
              <ul class="navbar-nav">
                <li class="nav-item">
                  <a class="nav-link active" aria-current="page" href="/">Home</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link active" aria-current="page" href="/categories">Categories</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link active" aria-current="page" href="#">Offers</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="#">Wallet</a>
                </li>
              </ul>
              <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                <div class="d-flex flex-row justify-content-end gap-10">
                  <span class="navlink-container">
                    <li class="nav-item">
                      <a class="nav-link" href="#">About Us</a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" href="#">Contact Us</a>
                    </li>
                    <div class="d-flex flex-row justify-content-end gap-2">
                      <li class="nav-item dropdown">
                        <div class="nav-link dropdown-toggle" role="button" id="accountDropdown" data-bs-toggle="dropdown" aria-expanded="false">
                          <span class="nav-user-img">
                            <img style="width: 40px; height: 40px; border-radius: 50%;" src="/images/admin.jpg" alt="User name">
                              <span class="online-dot"></span>
                          </span>
                        </div>
                          {{#if userData}}
                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="accountDropdown">
                              <li><a class="dropdown-item" href="##">{{userData.userName}}</a></li>
                              <li><a class="dropdown-item" href="##">Account Settings</a></li>
                              <li><a class="dropdown-item" href="/logout">Logout</a></li>
                            </ul>
                          {{else}}
                            <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="accountDropdown">
                              <li><a class="dropdown-item" href="/login">LogIn</a></li>
                            </ul>
                          {{/if}}
                      </li>
                      <li class="nav-item">
                        <a href="#" type="button" class="btn  btn-small">
                          <i class="bi bi-heart-fill"></i>
                        </a>
                      </li>
                      <li class="nav-item">
                        <a href="/cart" type="button" class="btn  btn-small">
                          <i class="bi bi-cart-fill"></i>
                        </a>
                          <!-- Display cart count or other information -->
                          {{!-- <span>{{ userCart.items.length }}</span> --}}
                      </li>
                    </div>
                  </div>
                </ul>
            </div>
        </div>
      </div>
    </div>
  </nav>

  </header>

<main>
  {{{body}}}
</main>
