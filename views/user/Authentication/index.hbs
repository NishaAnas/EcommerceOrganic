            {{#if success}}
            <div id="flashMessage" class="alert alert-success" role="alert">
              {{success}}
            </div>
            {{/if}}
  <div class="container-fluid ">
    <main>
      <section class="website-image row ">
        <div class="col-lg-6 d-flex flex-column align-items-center">
          <img class="branch-icon " loading="lazy" alt="image" src="/images/organic-products.png" />
        </div>
        <div
          class="col-lg-6 col-md-8 col-sm-10 d-flex flex-column align-items-center">
          <div class="conditional-link text-center ">

            <div class="comparison-node">
              <div class="iteration-loop">
                <div class="sequence-flow">
                  <div class="parallel-split">
                    <img class="logo-leaf-new-1-icon" loading="lazy" alt="images" src="/images/logo-leaf-new.png" />
                  </div>
                  <div class="conditional-link d-flex flex-column align-items-center">
                    <div class="best-quality-products">
                      Best Quality Products
                    </div>
                    <h1 class="join-the-organic">Join The Organic Movement!</h1>
                  </div>
