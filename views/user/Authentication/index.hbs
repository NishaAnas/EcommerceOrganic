                  <a href="/categories">
                  <button class="flow-control ">
                    <div class="rectangle"></div>
                    <div class="shop-now">Shop Now</div>
                  </button>
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <section class="index-tracker ">
        <div class="boolean-operator">
          <div class="rectangle rectangle1 "></div>
          <div class="nested-loop col-lg-3 col-md-6 col-sm-9">
            <div class="rectangle rectangle2 col-lg-2 col-md-6 col-sm-12"></div>
            <div class="free-shipping">Free Shipping</div>
            <div class="above-5-only">Above $5 Only</div>
          </div>
          <div class="nested-loop1 ">
            <div class="rectangle rectangle3 col-lg-2 col-md-6 col-sm-12"></div>
            <div class="certified-organic">Certified Organic</div>
            <div class="guarantee">100% Guarantee</div>
          </div>
          <div class="nested-loop2 ">
            <div class="rectangle rectangle4 col-lg-2 col-md-6 col-sm-12"></div>
            <div class="huge-savings">Huge Savings</div>
            <div class="at-lowest-price">At Lowest Price</div>
          </div>
          <div class="nested-loop3 ">
            <div class="rectangle rectangle5 col-lg-2 col-md-6 col-sm-12"></div>
            <div class="easy-returns">Easy Returns</div>
            <div class="no-questions-asked">No Questions Asked</div>
          </div>
        </div>
      </section>
      <section class="input-output">
  <div class="data-processor">
      <div class="graph-connection ">
        <h1 class="best-selling-products">Best Selling Products</h1>
        <div class="text-manipulator">
                  <img class="geometry-tools-icon" loading="lazy" alt
                    src="/images/logo-leaf-new.png" />
                </div>
        <div class="card-aligner">
        
          {{#each products}}
          <div class="card alignment-helper" style="width: 18rem;">
          <img
            src="{{this.images.[0]}}"
            class="card-img-top" alt=" {{this.attributeValue}}">
          <div class="card-body image-icon">
            <h5 class="card-title assorted-coffee"> {{this.attributeValue}}</h5>
            <p class="card-text frame-container"></p>
            <div class="error-handler d-flex justify-content-center align-items-center">
              <button class="flow-control ">
                <div class="rectangle"></div>
                <a href="/productDetails/{{this._id}}">
                <div class="shop-now">Shop Now</div>
                </a>
              </button>
            </div>
          </div>
        </div>
        {{/each}}
        </a>
        </div>
      </div>
  </div>
</section>

        <section class="vector-graphic">
